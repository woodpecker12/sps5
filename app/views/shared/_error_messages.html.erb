<% if @key.invalid? %>
    <div id="error_explanation">
      <div class="alert alert-error">
        The form contains <%= pluralize(@key.errors.count, "error") %>.
      </div>
      <ul>
        <% @key.errors.full_messages.each do |msg| %>
            <li>* <%= msg %></li>
        <% end %>
      </ul>
    </div>
<% end %>